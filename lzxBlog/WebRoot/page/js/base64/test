<script type="text/javascript">
	
function base64_encode(){		//编码
	var str=CryptoJS.enc.Utf8.parse($("#ts1").html());
	var base64=CryptoJS.enc.Base64.stringify(str);
	$("#ts2").html(base64);
	alert(base64);
}
function base64_decode(){		//解码
	var words  = CryptoJS.enc.Base64.parse($("#ts2").html());
	$("#ts3").html(words.toString(CryptoJS.enc.Utf8));
}


